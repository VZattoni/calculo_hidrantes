
Sub reserva()

'Célula do Tipo do hid (Planilha2)'

linha = 3
coluna = 3

'Célula da Área (Planilha2)'

linhaA = 5
colunaA = 3

'Célula do Material (Planilha2)'

linhaM = 4
colunaM = 3

'Célula do Reservatório (Planilha2)'

linha_res = 8
coluna_res = 3


'Worksheets("Planilha2").Cells(8, 19) --> Célula Hpm'


Select Case Worksheets("Planilha2").Cells(linha, coluna)

Case 1


    Formata_Tabela
    Formata_Tabela2

    Worksheets("Planilha1").Range("K5").Value = 100
    
    Worksheets("Planilha2").Range("C11").Interior.ColorIndex = 15
    
        If Worksheets("Planilha2").Range("C3").Value = 1 Then

        Worksheets("Planilha2").Range("C11").Value = 25

    End If

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
 
    With Worksheets("Planilha1").Range("A67").Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("A67:H67").Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("A67:H67").Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("H67").Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    

    
 ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
 
    With Worksheets("Planilha1").Range("B77:B86").Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("B77").Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("B86").Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
    
    With Worksheets("Planilha1").Range("B77:B86").Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .Weight = xlThick
        .ColorIndex = 3
    End With
 
 
 '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    

   If Worksheets("Planilha2").Cells(linhaA, colunaA) <= 2500 Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 3)
   ElseIf (Worksheets("Planilha2").Cells(linhaA, colunaA) > 2500 And Worksheets("Planilha2").Cells(linhaA, colunaA) <= 5000) Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 4)
   ElseIf (Worksheets("Planilha2").Cells(linhaA, colunaA) > 5000 And Worksheets("Planilha2").Cells(linhaA, colunaA) <= 10000) Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 5)
   ElseIf (Worksheets("Planilha2").Cells(linhaA, colunaA) > 10000 And Worksheets("Planilha2").Cells(linhaA, colunaA) <= 20000) Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 6)
   ElseIf (Worksheets("Planilha2").Cells(linhaA, colunaA) > 20000 And Worksheets("Planilha2").Cells(linhaA, colunaA) <= 50000) Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 7)
   ElseIf Worksheets("Planilha2").Cells(linhaA, colunaA) > 50000 Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(17, 8)
   End If
   
   Worksheets("Planilha2").Cells(9, 3) = 100
   
   
   
   
   
   
Case 2


    Formata_Tabela
    Formata_Tabela2

    Worksheets("Planilha1").Range("K5").Value = 150
    
    Worksheets("Planilha2").Range("C11").Interior.ColorIndex = 15
    
        If Worksheets("Planilha2").Range("C3").Value = 2 Then

        Worksheets("Planilha2").Range("C11").Value = 40

    End If

 
