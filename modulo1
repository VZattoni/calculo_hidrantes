   End If
   
   Worksheets("Planilha2").Cells(9, 3) = 200
   Worksheets("Planilha2").Cells(8, 19) = Worksheets("Tabelas").Cells(57, 34)
   
   
   
   
   
   
   
   
   
   
   
   
