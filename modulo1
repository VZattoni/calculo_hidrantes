       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(19, 6)
   ElseIf (Worksheets("Planilha2").Cells(linhaA, colunaA) > 20000 And Worksheets("Planilha2").Cells(linhaA, colunaA) <= 50000) Then
       Worksheets("Planilha2").Cells(linha_res, coluna_res) = Worksheets("Tabelas").Cells(19, 7)
   ElseIf Worksheets("Planilha2").Cells(linha, colunaA) > 50000 Then
