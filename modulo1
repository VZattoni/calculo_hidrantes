Dim conv
Dim vazao As String
Dim k As String
Dim vazao1 As String
Dim Dc1 As String
Dim Dm As String
Dim pi As String
Dim conv1
Dim Vv As String
Dim tmp
Dim tmp1
Dim tmp2
Dim tmp3
Dim hpc As String
Dim hpm As String
Dim hpv As String
Dim hpesg As String
Dim sum As String
Dim hpa As String
Dim ph As String
Dim vazao2
Dim vazao3 As String
Dim conexoes As String

Dim P As String
Dim Hg As String
Dim Hpri As String
Dim Hpma As String

Dim P_aux As String
Dim Hg_aux As String
Dim Hpri_aux As String
Dim Hpma_aux As String

conv = Worksheets("Planilha2").Range("c10").Value 'Convertendo a variavel para int para poder usand Round()

conexoes = Worksheets("Planilha2").Range("G27").Value
vazao = Worksheets("Planilha1").Range("K5").Value
k = Worksheets("Planilha1").Range("P34").Value
vazao1 = Round(conv, 4)
Dc1 = Worksheets("Planilha2").Range("C2").Value / 1000
Dm = Worksheets("Planilha2").Range("c11").Value
pi = Worksheets("Tabelas").Range("M4").Value
conv1 = Worksheets("Planilha1").Range("J65").Value
Vv = Round(conv1, 2)
tmp = Worksheets("Planilha1").Range("J57").Value
hpc = Round(tmp, 3)
hpm = Worksheets("Planilha1").Range("J59").Value
tmp1 = Worksheets("Planilha1").Range("N64").Value
hpv = Round(tmp1, 2)
hpesg = Worksheets("Planilha1").Range("J68").Value
sum = "+"
tmp2 = Worksheets("Planilha1").Range("K72").Value
hpa = Round(tmp2, 2)
tmp3 = Worksheets("Planilha1").Range("N36").Value
ph = Round(tmp3, 2)
vazao2 = conv * 2
vazao3 = Round(vazao2, 4)

P_aux = Worksheets("Planilha2").Range("R20").Value
Hg_aux = Worksheets("Planilha2").Range("C6").Value
Hpri_aux = Worksheets("Planilha2").Range("X5").Value
Hpma_aux = Worksheets("Planilha2").Range("X7").Value

P = Round(P_aux, 2)
Hg = Round(Hg_aux, 2)
Hpri = Round(Hpri_aux, 2)
Hpma = Round(Hpma_aux, 2)


Worksheets("Planilha1").Range("J36").Value = "'" & "= (" + vazao + ")² / (" + k + ")²"

Worksheets("Planilha1").Range("J56").Value = "'" & "HPc = 10,65 x " + vazao1 + "^(" + "1,85" + ") " + "x 2,5 x 130^(-1,85) x " + Dc1 + "^(-4,87)"

Worksheets("Planilha1").Range("L59").Value = "'" & "(Valor tabelado para o diâmetro de " + Dm + "mm com vazão de " + vazao + " LPM)"

Worksheets("Planilha1").Range("J64").Value = "'" & "Vv = (" + vazao1 + " x 4) / (" + pi + " x " + Dc1 + "²)"

Worksheets("Planilha1").Range("N63").Value = "'" & "HPv = 5 x (" + Vv + ")² / 2 x 9,81"

Worksheets("Planilha1").Range("K71").Value = "'" & hpc + " " + sum + " " + hpv + " " + sum + " " + hpm + " " + sum + " " + hpesg

Worksheets("Planilha1").Range("J77").Value = "'" & "P = " + ph + " mca " + sum + " " + hpa + " mca"

Worksheets("Planilha1").Range("J84").Value = "'" & "Qcl = 2 x " + vazao

Worksheets("Planilha1").Range("J92").Value = "'" & "Hp = (10,65 x " + vazao3 + "^(1,85) x 2,5) / (130^(1,85) x " + Dc1 + "^(4,87))"

Worksheets("Planilha1").Range("S3").Value = "'" & "Hp = (10,65 x " + vazao3 + "^(1,85) x " + conexoes + ") / (130^(1,85) x " + Dc1 + "^(4,87))"

Worksheets("Planilha1").Range("S8").Value = "'" & "H = " + P + " + " + Hg + " + " + Hpri + " + " + Hpma

End Sub

Public Function Formata_Tabela()



    With Worksheets("Planilha1").Range("C65:E66").Borders()
    
        .LineStyle = xlContinuous
        .Weight = xlThin
